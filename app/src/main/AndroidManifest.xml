<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiayiye.jetpackstudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".savestate.SaveStateActivity"></activity>
        <activity android:name=".basketball.BasketballActivity" />
        <activity android:name=".livedata.LiveDataActivity" />
        <activity android:name=".viewmodel.ViewModelActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".databinding.DataBindingActivity" />
        <activity android:name=".savestatejava.SaveStateJavaActivity" />
    </application>

</manifest>